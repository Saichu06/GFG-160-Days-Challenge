class Solution {
    void pushZerosToEnd(int[] arr) {
        int left=0;
        
        for(int i=0;i<arr.length;i++){
            if(arr[i]!=0){
                int temp=arr[i];
                arr[i]=arr[left];
                arr[left]=temp;
                left++;
            }
        }
            //MOVING NON-ZERO
            
            //MOVING ZERO 
            // if(arr[left]==0){
            //     int temp=arr[left];
            //     arr[left]=arr[right];
            //     arr[right]=temp;
                
            //     right--;
            //     left++;
            // }
            // else{
            //     left++;
            // }
    }
}
