class Solution {
    public int getSecondLargest(int[] arr) {
        int c=0;
        int first=-1;
        int second=-1;
        if(arr.length<2) return 1;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>first){
                second=first;
                first=arr[i];
            }
            else if(arr[i]>second && arr[i]<first){
                second=arr[i];
            }
        }
        
        return second;
    }
}
