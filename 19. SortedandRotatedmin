// User function Template for Java

class Solution {
    public int findMin(int[] arr) {
        // int min=arr[0];
        // for(int i=0;i<arr.length;i++){
        //     min=Math.min(min , arr[i]);
        // }
        
        // return min;
        
        int left=0;
        int right = arr.length-1;
        
        while(left<right){
            if(arr[left]<arr[right]){
                return arr[left];
            }
            
            int mid=left+(right-left)/2;
            
            if(arr[mid]>arr[right]){
                left=mid+1;
            }
            else{
                right=mid;
            }
        }
        
        return arr[left];
    }
}
