class Solution {
    public void rearrangeQueue(Queue<Integer> q) {
        Queue<Integer> q1=new LinkedList<>();
        int half=q.size()/2;
        
        for(int i=0;i<half;i++){
            q1.add(q.remove());
        }
        
        while(!q1.isEmpty()){
            q.add(q1.remove());
            q.add(q.remove());
        }
    }
}
