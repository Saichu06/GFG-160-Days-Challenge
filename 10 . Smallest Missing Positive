Brute
class Solution {
    public int firstMissingPositive(int[] nums) {
        Arrays.sort(nums);
        int n=nums.length;
        int curr=1;
        for(int i=0;i<n;i++){
            if(curr==nums[i]){
                curr++;
            }
        }

        return curr;
    }
}

Better
class Solution {
    public int missingNumber(int[] nums) {
        Set<Integer> set=new HashSet<>();
        for(int num:nums){
            if(num>0) set.add(num);
        }
        
        int smallest=1;
        while(set.contains(smallest)){
            smallest++;
        }
        
        return smallest;
    }     
}



