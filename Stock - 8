// User function Template for Java

class Solution {
    public int maximumProfit(int prices[]) {
        // Code here
        if(prices.length==0) return 0;
        int max=0;
        int min=prices[0];
        for(int i=0;i<prices.length;i++){
            min=Math.min(prices[i],min);
            int sum=prices[i]-min;
            if(max<sum){
                max=sum;
            }
        }
        
        return max;
    }
}
